{
  "env": {
      "commonjs": true,
      "es6": true,
      "node": true,
      "jest/globals": true
  },
  "plugins": [
    "jest"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "sourceType": "module"
  },
  "rules": {
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/valid-expect": "error"
  }
}